{
  "name": "api",
  "version": "1.0.0-alpha.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test": "node ace test"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.0.1",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.2",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.6",
    "@adonisjs/limiter": "^1.0.1",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/redis": "^7.3.1",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/view": "^6.1.6",
    "adonis-lucid-soft-deletes": "^1.4.3",
    "is-port-reachable": "^3.1.0",
    "luxon": "^3.0.1",
    "mysql": "^2.18.1",
    "net": "^1.0.2",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  }
}
